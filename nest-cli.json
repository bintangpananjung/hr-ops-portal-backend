{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "deleteOutDir": true,
    "assets": [
      {
        "include": "generated/prisma/**/*",
        "root": "src",
        "outDir": "dist/src",
        "watchAssets": true
      }
    ],
    "watchAssets": true
  }
}
